    "updateRule": null,
    "deleteRule": null,
    "options": {
      "query": "select \n  concat(t.sprint, '-',t.date) as id,\n  t.sprint,\n  t.date,\n  s.points as tbd_points,\n  sum(t.points) as total_points,\n  sum(\n    case \n      when t.status = 'In Review' then 1\n      else 0\n    end\n  ) as cr_points,\n  sum(\n    case \n      when t.status = 'In Test' then 1\n      else 0\n    end\n  ) as to_val_points,\n  sum(\n    case \n      when t.status = 'Done' then 1\n      else 0\n    end\n  ) as done_points\nfrom tickets t\ninner join sprints_points_tbd_view s on s.sprint = t.sprint\ngroup by t.sprint, t.date"
    }
